	measure 'Nuevos Clientes Vendedor' = ```
			
			CALCULATE(
			    COUNTROWS(
			        FILTER(
			            DIM_CLIENTE,
			            VAR primer_compra = 
			                CALCULATE(
			                    MIN(FACT_VENTAS[fecha]),
			                    ALLEXCEPT(FACT_VENTAS, DIM_CLIENTE[nit])
			                )
			            RETURN
			                primer_compra >= MIN(DIM_TIEMPO[fecha])
			        )
			    ),
