	measure 'Crecimiento Vendedor' = ```
			
			VAR ventas_actual = [Ventas por Vendedor]
			VAR ventas_anterior = 
			    CALCULATE(
			        [Ventas por Vendedor],
			        SAMEPERIODLASTYEAR(DIM_TIEMPO[fecha])
			    )
			RETURN
			    DIVIDE(ventas_actual - ventas_anterior, ventas_anterior, 0)
