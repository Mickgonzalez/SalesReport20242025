	measure 'Top Marcas por Vendedor' = ```
			
			// Devuelve la marca principal de cada vendedor
			VAR marcas_vendedor = 
			    SUMMARIZE(
			        FILTER(
			            FACT_VENTAS,
			            [Ventas por Vendedor] > 0
			        ),
			        DIM_PRODUCTO[marca],
			        "Ventas", [Ventas Totales]
			    )
			RETURN
			    MAXX(TOPN(1, marcas_vendedor, [Ventas], DESC), [marca])
			```
		lineageTag: 602a0788-34bf-4e71-9dcb-68b8c1bf02f6

	measure '// Medida para gráfico específicoVentas para Gráfico Agrupado' =
			
			VAR vendedor_actual = SELECTEDVALUE(DIM_VENDEDOR[nom_vendedor])
			VAR marca_actual = SELECTEDVALUE(DIM_PRODUCTO[marca])
			VAR ano_actual = SELECTEDVALUE(DIM_TIEMPO[año])
			RETURN
			    CALCULATE(
			        [Ventas Totales],
			        DIM_VENDEDOR[nom_vendedor] = vendedor_actual,
			        DIM_PRODUCTO[marca] = marca_actual,
			        DIM_TIEMPO[año] = ano_actual
